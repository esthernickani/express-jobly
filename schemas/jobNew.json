{
    "$schema": "https://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "title": "Root Schema",
    "type": "object",
    "default": {},
    "required": [
        "title",
        "salary",
        "equity",
        "company_handle"
    ],
    "properties": {
        "title": {
            "title": "The title Schema",
            "type": "string",
            "default": "",
            "examples": [
                "Information officer"
            ]
        },
        "salary": {
            "title": "The salary Schema",
            "type": "number",
            "default": 0,
            "examples": [
                10000
            ]
        },
        "equity": {
            "title": "The equity Schema",
            "type": "string",
            "default": "",
            "examples": [
                "0"
            ]
        },
        "company_handle": {
            "title": "The company_handle Schema",
            "type": "string",
            "default": "",
            "examples": [
                "watson-davis"
            ]
        }
    },
    "examples": [{
        "title": "Information officer",
        "salary": 10000,
        "equity": "0",
        "company_handle": "watson-davis"
    }]
}  